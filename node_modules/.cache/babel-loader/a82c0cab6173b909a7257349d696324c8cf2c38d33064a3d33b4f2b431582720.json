ist(results) {\n      this.cityList = results;\n    },\n    apiCallCity(link, city_name) {\n      console.log(\"apiCallCitycalled with \" + link);\n      const result = [];\n      try {\n        axios.get(link).then(function (response) {\n          //console.log (response);\n          //console.log (JSON.stringify(response));\n\n          result.push({\n            temp: response.data.list[0].main.temp,\n            feels_like: response.data.list[0].main.feels_like,\n            temp_min: response.data.list[0].main.temp_min,\n            temp_max: response.data.list[0].main.temp_max,\n            pressure: response.data.list[0].main.pressure,\n            sea_level: response.data.list[0].main.sea_level,\n            grnd_level: response.data.list[0].main.grnd_level,\n            humidity: response.data.list[0].main.humidity,\n            temp_kf: response.data.list[0].main.temp_kf\n          });\n          this.tmp = response.data.list[0].main.temp;\n          this.feels_like = response.data.list[0].main.feels_like;\n          this.temp_min = response.data.list[0].main.temp_min;\n          this.temp_max = response.data.list[0].main.temp_max;\n          this.pressure = response.data.list[0].main.pressure;\n          this.sea_level = response.data.list[0].main.sea_level;\n          this.grnd_level = response.data.list[0].main.grnd_level;\n          this.humidity = response.data.list[0].main.humidity;\n          this.temp_kf = response.data.list[0].main.temp_kf;\n          this.weatherDetails = response.data.list[1].weather[0].main;\n          this.weatherDetailsIcon = \"http://openweathermap.org/img/w/\" + response.data.list[1].weather[0].icon + \".png\";\n          this.weatherData = result;\n          this.cityTitle = city_name;\n          this.showModal();\n        }.bind(this));\n      } catch (err) {\n        console.log(\"error: \" + err);\n      }\n    },\n    apiCallList(link) {\n      const result = [];\n      try {\n        axios.get(link).then(function (response) {\n          console.log(response);\n          console.log(JSON.stringify(response));\n          // console.log (\"response.data.city.nam= \" + response.data.city.name)\n          console.log(\"response.data length \" + response.data.length);\n          // console.log (\"response.data.city.nam= \" + response.data[0].name);\n          // console.log (\"response.data.city.nam= \" + response.data[1].name);\n          // console.log (\"response.data.city.nam= \" + response.data[2].name);\n\n          for (var i = 0; i < response.data.length; i++) {\n            result.push({\n              name: response.data[i].name,\n              country: response.data[i].country,\n              state: response.data[i].state,\n              lat: response.data[i].lat,\n              lon: response.data[i].lon\n            });\n          }\n          console.log(result);\n          this.getcityList(result);\n        }.bind(this));\n      } catch (err) {\n        console.log(\"error: \" + err);\n      }\n    },\n    // async apiCall() {\n    //   try {\n    //     const { data } = await this.$http.patch(\n    //       'http://api.openweathermap.org/data/2.5/forecast?id=524901&appid=8a81160d794a7c7ff46372e14aef240f', \n    //       { appid: \"8a81160d794a7c7ff46372e14aef240f\" }\n    //     );\n    //     console.log(\"data: \" + data);\n    //   } catch (err) {\n    //     console.log(\"error: \" + err);\n    //   }\n    // },\n\n    arrayOps(arr) {\n      const result = [];\n      const numRows = arr.length;\n      const numCols = arr[0].length;\n      for (let i = 0; i < numCols; i++) {\n        let sum = 0;\n        for (let j = 0; j < numRows; j++) {\n          sum += arr[j][i];\n        }\n        result.push(sum);\n      }\n      for (let i = 0; i < numRows; i++) {\n        let sum = 0;\n        for (let j = 0; j < numCols; j++) {\n          sum += arr[i][j];\n        }\n        result.push(sum);\n      }\n      return result;\n    },\n    isAnagram(first, second) {\n      /