onse.data[0].name);\n          // console.log (\"response.data.city.nam= \" + response.data[1].name);\n          // console.log (\"response.data.city.nam= \" + response.data[2].name);\n\n          for (var i = 0; i < response.data.length; i++) {\n            result.push({\n              name: response.data[i].name,\n              country: response.data[i].country,\n              state: response.data[i].state,\n              latlong: response.data[i].lat + ',' + response.data[i].lon\n            });\n          }\n          console.log(result);\n          this.getcityList(result);\n        }.bind(this));\n      } catch (err) {\n        console.log(\"error: \" + err);\n      }\n    },\n    // async apiCall() {\n    //   try {\n    //     const { data } = await this.$http.patch(\n    //       'http://api.openweathermap.org/data/2.5/forecast?id=524901&appid=8a81160d794a7c7ff46372e14aef240f', \n    //       { appid: \"8a81160d794a7c7ff46372e14aef240f\" }\n    //     );\n    //     console.log(\"data: \" + data);\n    //   } catch (err) {\n    //     console.log(\"error: \" + err);\n    //   }\n    // },\n\n    arrayOps(arr) {\n      const result = [];\n      const numRows = arr.length;\n      const numCols = arr[0].length;\n      for (let i = 0; i < numCols; i++) {\n        let sum = 0;\n        for (let j = 0; j < numRows; j++) {\n          sum += arr[j][i];\n        }\n        result.push(sum);\n      }\n      for (let i = 0; i < numRows; i++) {\n        let sum = 0;\n        for (let j = 0; j < numCols; j++) {\n          sum += arr[i][j];\n        }\n        result.push(sum);\n      }\n      return result;\n    },\n    isAnagram(first, second) {\n      // Convert strings to lowercase and remove any non-alphabetic characters\n      const regex = /[^\\w]/g;\n      const str1 = first.toLowerCase().replace(regex, '');\n      const str2 = second.toLowerCase().replace(regex, '');\n\n      // Sort the characters in each string and compare them\n      const sortedStr1 = str1.split('').sort().join('');\n      const sortedStr2 = str2.split('').sort().join('');\n      return sortedStr1 === sortedStr2;\n    }\n  }\n};","map":{"version":3,"names":["axios","name","components","cityModal","data","city","cityList","isModalVisible","created","console","log","methods","showModal","closeModal","searchCity","callApi","getcityList","results","link","result","get","then","response","JSON","stringify","length","i","push","country","state","latlong","lat","lon","bind","err","arrayOps","arr","numRows","numCols","sum","j","isAnagram","first","second","regex","str1","toLowerCase","replace","str2","sortedStr1","split","sort","join","sortedStr2"],"sources":["/home/itsampis/Documents/vuejs/demoapp/src/App.vue"],"sourcesContent":["\n\n<template>\n\n\n<div class=\"input-group\">\n  <input type=\"search\" class=\"form-control rounded\"\n          placeholder=\"Search the city...\" aria-label=\"Search\" \n          aria-describedby=\"search-addon\" v-model=\"city\" @keyup.enter=\"searchCity\"/>\n  <button type=\"button\" class=\"btn btn-outline-primary\" @click =\"searchCity\">Search</button>\n</div>\n\n<table id=\"list-view\" v-for=\"city in cityList\" :key=\"city\">\n  <tr>\n    <td>{{city.name}} <span class=\"country-code\">{{city.state}} ({{city.country}})</span></td>\n  </tr>\n</table>\n  \n\n <div class=\"item error\" v-if=\"input&&!filteredList().length\">\n    <p>No results found!</p>\n </div>\n\n\n\n<cityModal\n  v-show=\"isModalVisible\"\n  @close=\"closeModal\"\n>\n  <template v-slot:header>\n    This is a new modal header.\n  </template>\n\n  <template v-slot:body>\n    This is a new modal body.\n  </template>\n\n  <template v-slot:footer>\n    This is a new modal footer.\n  </template>\n</cityModal>\n\n<div id=\"app\">\n  <button\n    type=\"button\"\n    class=\"btn\"\n    @click=\"showModal\"\n  >\n    Open Modal!\n  </button>\n\n  <Modal\n    v-show=\"isModalVisible\"\n    @close=\"closeModal\"\n  />\n</div>\n</template>\n\n\n\n<script>\n\n//impor