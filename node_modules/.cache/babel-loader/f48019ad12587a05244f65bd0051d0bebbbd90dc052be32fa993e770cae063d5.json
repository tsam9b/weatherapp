sole.log (\"response.data.city.nam= \" + response.data[1].name);\n          // console.log (\"response.data.city.nam= \" + response.data[2].name);\n\n          for (var i=0;i < response.data.length; i++){\n            result.push({name: response.data[i].name,country: response.data[i].country, state: response.data[i].state, latlong: response.data[i].lat+ ',' + response.data[i].lon } );\n          }\n\n          console.log(result);\n          this.getcityList(result);\n        }.bind(this));\n      }\n      catch (err) {\n        console.log(\"error: \" + err);\n      }\n    },\n\n    // async apiCall() {\n    //   try {\n    //     const { data } = await this.$http.patch(\n    //       'http://api.openweathermap.org/data/2.5/forecast?id=524901&appid=8a81160d794a7c7ff46372e14aef240f', \n    //       { appid: \"8a81160d794a7c7ff46372e14aef240f\" }\n    //     );\n    //     console.log(\"data: \" + data);\n    //   } catch (err) {\n    //     console.log(\"error: \" + err);\n    //   }\n    // },\n\n    arrayOps(arr) {\n      const result = [];\n      const numRows = arr.length;\n      const numCols = arr[0].length;\n      \n      for (let i = 0; i < numCols; i++) {\n        let sum = 0;\n        for (let j = 0; j < numRows; j++) {\n          sum += arr[j][i];\n        }\n        result.push(sum);\n      }\n      \n      for (let i = 0; i < numRows; i++) {\n        let sum = 0;\n        for (let j = 0; j < numCols; j++) {\n          sum += arr[i][j];\n        }\n        result.push(sum);\n      }\n      \n      return result;\n    },\n\n    isAnagram(first, second) {\n      // Convert strings to lowercase and remove any non-alphabetic characters\n      const regex = /[^\\w]/g;\n      const str1 = first.toLowerCase().replace(regex, '');\n      const str2 = second.toLowerCase().replace(regex, '');\n\n      // Sort the characters in each string and compare them\n      const sortedStr1 = str1.split('').sort().join('');\n      const sortedStr2 = str2.split('').sort().join('');\n\n      return sortedStr1 === sortedStr2;\n    }\n\n\n  }\n\n}\n</script>\n\n<style>@import './css/style.css';</style>\n\n\n\n\n\n\n\n\n<!-- <template>\n  <img alt=\"Vue logo\" src=\"./assets/logo.png\">\n  <HelloWorld msg=\"Welcome to Your Vue.js App\"/>\n</template>\n\n<script>\nimport HelloWorld from './components/HelloWorld.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    HelloWorld\n  } -->\n<!-- }\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style> --> -->\n"],"mappings":";AA8DA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,sCAAsC;AAC7C,OAAO,kCAAkC;AAGzC,eAAe;EACbC,IAAI,EAAE,KAAK;EACTC,UAAU,EAAE;IACVC;EACJ,CAAC;EACDC,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EAAE;MACPC,QAAQ,EAAC,EAAE;MACXC,cAAc,EAAE;IAClB;EAEF,CAAC;EAEDC,OAAMA,CAAA,EAAI;IACRC,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;IACtB;IACA;;;;;IAKA;EAEF,CAAC;;EAEDC,OAAO,EAAE;IAEPC,SAASA,CAAA,EAAG;MACR,IAAI,CAACL,cAAa,GAAI,IAAI;IAC9B,CAAC;IACDM,UAAUA,CAAA,EAAG;MACX,IAAI,CAACN,cAAa,GAAI,KAAK;IAC7B,CAAC;IAEDO,UAAUA,CAAA,EAAE;MACVL,OAAO,CAACC,GAAG,CAAC,0BAAyB,GAAI,IAAI,CAACL,IAAI,CAAC;MACnD,IAAI,CAACU,OAAO,CAAC,iDAAiD,GAAE,IAAI,CAACV,IAAG,GAAG,kDAAkD;IAE/H,CAAC;IAEDW,WAAWA,CAACC,OAAO,EAAC;MAClB,IAAI,CAACX,QAAO,GAAIW,OAAO;IACzB,CAAC;IAEDF,OAAOA,CAACG,IAAI,EAAE;MACZ,MAAMC,MAAK,GAAI,EAAE;MACjB,IAAI;QACFnB,KAAK,CAACoB,GAAG,CAACF,IAAI,EACbG,IAAI,CAAC,UAAUC,QAAQ,EAAE;UACxBb,OAAO,CAACC,GAAE,CAAGY,QAAQ,CAAC;UACtBb,OAAO,CAACC,GAAE,CAAGa,IAAI,CAACC,SAAS,CAACF,QAAQ,CAAC,CAAC;UACtC;UACAb,OAAO,CAACC,GAAE,CAAG,uBAAsB,GAAIY,QAAQ,CAAClB,IAAI,CAACqB,MAAM,CAAC;UAC5D;UACA;UACA;;UAEA,KAAK,IAAIC,CAAC,GAAC,CAAC,EAACA,CAAA,GAAIJ,QAAQ,CAAClB,IAAI,CAACqB,MAAM,EAAEC,CAAC,EAAE,EAAC;YACzCP,MAAM,CAACQ,IAAI,CAAC;cAAC1B,IAAI,EAAEqB,QAAQ,CAAClB,IAAI,CAACsB,CAAC,CAAC,CAACzB,IAAI;cAAC2B,OAAO,EAAEN,QAAQ,CAAClB,IAAI,CAACsB,CAAC,CAAC,